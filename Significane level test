# Significance level test alpha = 0.05

iter = 10000
n = 100
mu = 5
sd = 1
alpha = 0.05
t_value = qt(1 - alpha, df = n - 1)

result = rep(NA, iter)
for(i in 1:iter)
{
   set.seed(i)
   x = rnorm(n, mean = mu, sd = sd)
   se = sd(x) / sqrt(n)
   t = (mean(x) - 5) / se
   result[i] = (t > t_value)
}

mean(result)
